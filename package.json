{
  "name": "binary-structures",
  "version": "1.3.1",
  "description": "Yet another declarative binary packer/parser, but built for modern browsers.",
  "keywords": [
    "binary",
    "struct",
    "parse",
    "parser",
    "pack",
    "unpack",
    "decode",
    "encode",
    "ArrayBuffer",
    "bit",
    "byte"
  ],
  "main": "./dist/cjs-bundle.js",
  "module": "./dist/es-bundle.js",
  "typings": "./typings/index.d.ts",
  "directories": {
    "test": "__test__"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "improved-map": "^1.0.7",
    "jest": "^21.2.1",
    "rollup": "^0.52.2",
    "text-encoding": "^0.6.4",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "rollup": "rollup -c",
    "build": "npm run tsc && npm run rollup"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./jest_setup.js"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true,
        "tsConfigFile": "./__test__/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riggs/binary-structures.git"
  },
  "author": "Benjamin Riggs (https://github.com/riggs/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/riggs/binary-structures/issues"
  },
  "homepage": "https://github.com/riggs/binary-structures#readme",
  "files": [
    "src/*.ts",
    "dist",
    "typings/*.d.ts"
  ]
}
