{
  "name": "declarative-binary-serialization",
  "version": "0.9.0",
  "description": "Does what it says on the tin.",
  "main": "./dist/index.js",
  "typings": "./typings/index.d.ts",
  "directories": {
    "test": "__test__"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "improved_map": "^1.0.1",
    "jest": "^21.2.1",
    "text-encoding": "^0.6.4",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./jest_setup.js"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true,
        "tsConfigFile": "./__test__/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riggs/declarative-binary-serialization.git"
  },
  "author": "Benjamin Riggs (https://github.com/riggs/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/riggs/declarative-binary-serialization/issues"
  },
  "homepage": "https://github.com/riggs/declarative-binary-serialization#readme"
}
